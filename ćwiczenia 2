create table dbo.wydzialy
	(ID INT PRIMARY KEY IDENTITY, Akronim VARCHAR (4) NOT NULL, Wydzial VARCHAR(30) NOT NULL, CONSTRAINT SprawdzamAkronim CHECK (Akronim IN ('ZIE')))
	
